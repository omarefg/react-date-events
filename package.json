{
  "name": "@omarefg/react-date-events",
  "version": "0.2.3",
  "description": "React component to create a calendar and add reminders to it.",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.dev.js",
    "build": "rm -rf ./lib && webpack --mode production",
    "publish": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omarefg/react-date-events.git"
  },
  "keywords": [
    "react",
    "date",
    "calendar",
    "events",
    "react-dates",
    "react-calendar",
    "react-events"
  ],
  "author": "Omar Flores Grimontt <omarefg92@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omarefg/react-date-events/issues"
  },
  "homepage": "https://github.com/omarefg/react-date-events#readme",
  "dependencies": {
    "@date-io/date-fns": "1.3.11",
    "@material-ui/core": "^4.6.0",
    "@material-ui/pickers": "3.2.5",
    "classnames": "^2.2.6",
    "date-fns": "2.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.11.0"
  },
