    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "react-redux": "^7.1.3",
    "react-test-renderer": "^16.11.0",
    "redux": "^4.0.4",
    "sass-loader": "^8.0.0",
